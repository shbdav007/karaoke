{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Karaoke App",
      "program": "${workspaceFolder}/src/components/${input:entryFile}",
      "args": [
        "--serve"
      ]
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "entryFile",
      "description": "Select the entry file for your application",
      "options": [
        "App.js",
        "Auth/Login.js",
        "Auth/Register.js",
        "Catalog/SongCatalog.js",
        "Home/HomePage.js",
        "Player/KaraokePlayer.js",
        "Player/LyricsSync.js",
        "Profile/UserProfile.js",
        "Settings/SettingsPage.js",
        "Streaming/LiveStream.js",
        "Subscription/Subscription.js"
      ]
    }
  ]
}